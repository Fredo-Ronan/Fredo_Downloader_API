{
  "version": 2,
  "routes": [
    {
      "src": "/api/youtube-download",
      "dest": "youtube.js"
    },
    {
      "src": "/api/instagram-download",
      "dest": "instagram.js"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
